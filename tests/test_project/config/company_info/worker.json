{
    "fetcher": {
        "interval": 3,
        "timeout": 10,
        "retry_count": 3
    },
    "redis_queue": {
        "queue_id": "eastmoney:company_info"
    },
    "pipelines": {
        "parser": {
        },
        "file_storer": {
            "out_root_path": "./temp/company_info/file"
        },
        "sqlite_storer": {
            "out_root_path": "./temp/company_info/sqlite3"
        }
    },
    "worker": {
        "interval": 3,
        "init_error_to_todo": true,
        "recheck_count": 3,
        "recheck_sleep": 1
    },
    "logger": {
        "name": "eastmoney:company_info:worker",
        "console_level": "DEBUG",
        "filename": "./log/eastmoney/company_info_w.log",
        "by_date": true
    }
}
